
<div id="report-title"><h1>Reclamation</h1></div>
<table className="table table-sm table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Id Client</th>
            <th>Id Unite Location</th>
            <th>Id Dossierlocation</th>
            <th>User Id</th>
            <th>Type</th>
            <th>Description</th>
            <th>Status</th>
            <th>Date Created</th>
            <th>Date Updated</th>
            <th>Date Resolue</th>
        </tr>
    </thead>
    <tbody>
        <% records.forEach(function(record){ %>
        <tr>
            <td><%= record.id %></td>
            <td><%= record.id_client %></td>
            <td><%= record.id_unite_location %></td>
            <td><%= record.id_dossierlocation %></td>
            <td><%= record.user_id %></td>
            <td><%= record.type %></td>
            <td><%= record.description %></td>
            <td><%= record.status %></td>
            <td><%= record.date_created %></td>
            <td><%= record.date_updated %></td>
            <td><%= record.date_resolue %></td>
        </tr>
        <% }); %>
    </tbody>
</table>
