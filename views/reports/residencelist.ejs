
<div id="report-title"><h1>Residence</h1></div>
<table className="table table-sm table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Code</th>
            <th>Denomination</th>
            <th>Dt Mise Service</th>
            <th>Adresse</th>
            <th>Tel1</th>
            <th>Tel2</th>
            <th>Tel3</th>
            <th>Id Responsable</th>
            <th>Id Proprietaire</th>
        </tr>
    </thead>
    <tbody>
        <% records.forEach(function(record){ %>
        <tr>
            <td><%= record.id %></td>
            <td><%= record.code %></td>
            <td><%= record.denomination %></td>
            <td><%= record.dt_mise_service %></td>
            <td><%= record.adresse %></td>
            <td><%= record.tel1 %></td>
            <td><%= record.tel2 %></td>
            <td><%= record.tel3 %></td>
            <td><%= record.id_responsable %></td>
            <td><%= record.id_proprietaire %></td>
        </tr>
        <% }); %>
    </tbody>
</table>
