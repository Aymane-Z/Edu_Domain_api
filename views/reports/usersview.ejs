
<div id="report-title"><h1>User Details</h1></div>
<table className="table table-sm table-striped">
    <tbody>
        <tr>
            <th>Id</th>
            <td><%= record.id %></td>
        </tr>
        <tr>
            <th>User Id</th>
            <td><%= record.user_id %></td>
        </tr>
        <tr>
            <th>Username</th>
            <td><%= record.username %></td>
        </tr>
        <tr>
            <th>Email</th>
            <td><%= record.email %></td>
        </tr>
        <tr>
            <th>Date Registered</th>
            <td><%= record.date_registered %></td>
        </tr>
        <tr>
            <th>Photo</th>
            <td><%= record.photo %></td>
        </tr>
        <tr>
            <th>User Role</th>
            <td><%= record.user_role %></td>
        </tr>
        <tr>
            <th>Date Created</th>
            <td><%= record.date_created %></td>
        </tr>
        <tr>
            <th>Date Updated</th>
            <td><%= record.date_updated %></td>
        </tr>
        <tr>
            <th>User Role Id</th>
            <td><%= record.user_role_id %></td>
        </tr>
    </tbody>
</table>
