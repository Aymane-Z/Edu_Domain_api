
<div id="report-title"><h1>Garant</h1></div>
<table className="table table-sm table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Code</th>
            <th>Denomination</th>
            <th>Gerant</th>
            <th>Cin</th>
            <th>Adresse</th>
            <th>Rc</th>
            <th>Patente</th>
            <th>Tel1</th>
            <th>Tel2</th>
            <th>Tel3</th>
            <th>Mail</th>
            <th>Compte Bancaire</th>
            <th>Profession</th>
            <th>Revenus Mensuels</th>
            <th>Date Created</th>
            <th>Date Updated</th>
        </tr>
    </thead>
    <tbody>
        <% records.forEach(function(record){ %>
        <tr>
            <td><%= record.id %></td>
            <td><%= record.code %></td>
            <td><%= record.denomination %></td>
            <td><%= record.gerant %></td>
            <td><%= record.cin %></td>
            <td><%= record.adresse %></td>
            <td><%= record.rc %></td>
            <td><%= record.patente %></td>
            <td><%= record.tel1 %></td>
            <td><%= record.tel2 %></td>
            <td><%= record.tel3 %></td>
            <td><%= record.mail %></td>
            <td><%= record.compte_bancaire %></td>
            <td><%= record.profession %></td>
            <td><%= record.revenus_mensuels %></td>
            <td><%= record.date_created %></td>
            <td><%= record.date_updated %></td>
        </tr>
        <% }); %>
    </tbody>
</table>
