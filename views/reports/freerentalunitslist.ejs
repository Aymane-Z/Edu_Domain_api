
<div id="report-title"><h1>Freerentalunits</h1></div>
<table className="table table-sm table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Code</th>
            <th>Designation</th>
            <th>Typeunit</th>
            <th>Description</th>
            <th>Etat Physique</th>
            <th>Observation</th>
            <th>Etat</th>
            <th>Type</th>
            <th>Prix</th>
            <th>Photo</th>
            <th>Categoriechambre</th>
            <th>Id Typech</th>
            <th>Id Residence</th>
        </tr>
    </thead>
    <tbody>
        <% records.forEach(function(record){ %>
        <tr>
            <td><%= record.id %></td>
            <td><%= record.code %></td>
            <td><%= record.designation %></td>
            <td><%= record.typeunit %></td>
            <td><%= record.description %></td>
            <td><%= record.etat_physique %></td>
            <td><%= record.observation %></td>
            <td><%= record.etat %></td>
            <td><%= record.type %></td>
            <td><%= record.prix %></td>
            <td><%= record.photo %></td>
            <td><%= record.categoriechambre %></td>
            <td><%= record.id_typech %></td>
            <td><%= record.id_residence %></td>
        </tr>
        <% }); %>
    </tbody>
</table>
