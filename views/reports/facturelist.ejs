
<div id="report-title"><h1>Facture</h1></div>
<table className="table table-sm table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Code</th>
            <th>Dt Facture</th>
            <th>Id Facturation</th>
            <th>Date Created</th>
            <th>Date Updated</th>
        </tr>
    </thead>
    <tbody>
        <% records.forEach(function(record){ %>
        <tr>
            <td><%= record.id %></td>
            <td><%= record.code %></td>
            <td><%= record.dt_facture %></td>
            <td><%= record.id_facturation %></td>
            <td><%= record.date_created %></td>
            <td><%= record.date_updated %></td>
        </tr>
        <% }); %>
    </tbody>
</table>
