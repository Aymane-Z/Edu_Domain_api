
<div id="report-title"><h1>Lecture Des Index</h1></div>
<table className="table table-sm table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Date</th>
            <th>Mois Consommation</th>
            <th>Index Eau Froid</th>
            <th>Index Eau Chaud</th>
            <th>Index Electricite</th>
            <th>Observation</th>
            <th>Id Client</th>
            <th>Id Unite Location</th>
            <th>Date Created</th>
            <th>Date Updated</th>
        </tr>
    </thead>
    <tbody>
        <% records.forEach(function(record){ %>
        <tr>
            <td><%= record.id %></td>
            <td><%= record.date %></td>
            <td><%= record.mois_consommation %></td>
            <td><%= record.index_eau_froid %></td>
            <td><%= record.index_eau_chaud %></td>
            <td><%= record.index_electricite %></td>
            <td><%= record.observation %></td>
            <td><%= record.id_client %></td>
            <td><%= record.id_unite_location %></td>
            <td><%= record.date_created %></td>
            <td><%= record.date_updated %></td>
        </tr>
        <% }); %>
    </tbody>
</table>
