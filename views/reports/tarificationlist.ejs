
<div id="report-title"><h1>Tarification</h1></div>
<table className="table table-sm table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Dt Tarif</th>
            <th>Id Base Tarif</th>
            <th>Id Unite Location</th>
            <th>Date Created</th>
            <th>Date Updated</th>
        </tr>
    </thead>
    <tbody>
        <% records.forEach(function(record){ %>
        <tr>
            <td><%= record.id %></td>
            <td><%= record.dt_tarif %></td>
            <td><%= record.id_base_tarif %></td>
            <td><%= record.id_unite_location %></td>
            <td><%= record.date_created %></td>
            <td><%= record.date_updated %></td>
        </tr>
        <% }); %>
    </tbody>
</table>
