
<div id="report-title"><h1>Lignes Facture</h1></div>
<table className="table table-sm table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Code</th>
            <th>Designation</th>
            <th>Description</th>
            <th>Montant</th>
            <th>Id Facture</th>
            <th>Id Base Tarif</th>
            <th>Date Created</th>
            <th>Date Updated</th>
        </tr>
    </thead>
    <tbody>
        <% records.forEach(function(record){ %>
        <tr>
            <td><%= record.id %></td>
            <td><%= record.code %></td>
            <td><%= record.designation %></td>
            <td><%= record.description %></td>
            <td><%= record.montant %></td>
            <td><%= record.id_facture %></td>
            <td><%= record.id_base_tarif %></td>
            <td><%= record.date_created %></td>
            <td><%= record.date_updated %></td>
        </tr>
        <% }); %>
    </tbody>
</table>
