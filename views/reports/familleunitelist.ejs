
<div id="report-title"><h1>Famille Unite</h1></div>
<table className="table table-sm table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Code</th>
            <th>Designation</th>
            <th>Type</th>
            <th>Prix</th>
            <th>Photo</th>
        </tr>
    </thead>
    <tbody>
        <% records.forEach(function(record){ %>
        <tr>
            <td><%= record.id %></td>
            <td><%= record.code %></td>
            <td><%= record.designation %></td>
            <td><%= record.type %></td>
            <td><%= record.prix %></td>
            <td><%= record.photo %></td>
        </tr>
        <% }); %>
    </tbody>
</table>
