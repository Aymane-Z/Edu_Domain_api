
<div id="report-title"><h1>Usersnode</h1></div>
<table className="table table-sm table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Username</th>
            <th>Email</th>
            <th>Photo</th>
            <th>User Role Id</th>
            <th>Date Created</th>
            <th>Date Updated</th>
            <th>Civilite</th>
            <th>Nom</th>
            <th>Prenom</th>
            <th>Cin</th>
            <th>Nationalite</th>
            <th>Telephone</th>
            <th>Email Verified At</th>
        </tr>
    </thead>
    <tbody>
        <% records.forEach(function(record){ %>
        <tr>
            <td><%= record.id %></td>
            <td><%= record.username %></td>
            <td><%= record.email %></td>
            <td><%= record.photo %></td>
            <td><%= record.user_role_id %></td>
            <td><%= record.date_created %></td>
            <td><%= record.date_updated %></td>
            <td><%= record.civilite %></td>
            <td><%= record.nom %></td>
            <td><%= record.prenom %></td>
            <td><%= record.cin %></td>
            <td><%= record.nationalite %></td>
            <td><%= record.telephone %></td>
            <td><%= record.email_verified_at %></td>
        </tr>
        <% }); %>
    </tbody>
</table>
